<h1>Create Profile:</h1>

<form action="/users" method="post">
  <div class="container">
    <div class="form-group">
      <label for="full_name">*Full Name:</label>
      <input type="text" name="full_name" id="full_name" class="form-control" placeholder="John Doe">
    </div>
    <div class="form-group">
      <label for="provider_level">Choose a Provider Level:</label>
      <select id="provider_level" name="provider_level" class="form-control">
        <option>EMT-Basic</option>
        <option>EMT-Intermediate</option>
        <option>Paramedic</option>
      </select>
    </div>
    <div class="form-group">
      <label for="current_agency">Current Agency:</label>
      <input type="text" name="current_agency" id="current_agency" class="form-control" placeholder="ABC Ambulance">
    </div>
    <div class="form-group">
      <label for="email">*Email:</label>
      <input type="text" name="email" id="email" class="form-control" placeholder="someone@example.com">
    </div>
    <div class="form-group">
      <label for="username">*Create Username:</label>
      <input type="text" name="username" id="username" class="form-control" placeholder="John123">
    </div>
    <div class="form-group">
      <label for="password">*Create Password:</label>
      <input type="password" name="password" id="password" class="form-control" placeholder="********">
    </div>
    <p>(* Required)</p>
    <div>
      <button type="submit" class="btn btn-primary">Create Profile</button>
    </div>

  </div>
</form>
